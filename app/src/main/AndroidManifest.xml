<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.changmeen.firebase">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_icon_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">

        <activity android:name=".Ingredient_page"></activity>
        <activity android:name=".RecipeSearch"></activity>
        <activity android:name=".Profile_activity"></activity>
        <activity android:name=".MainActivity"></activity>
        <activity android:name=".SignUpActivity"></activity>
        <activity android:name=".receipeActivity"></activity>
        <activity android:name=".Ingredient_add"></activity>
        <activity android:name=".communityActivity"></activity>
        <activity android:name=".communitySearch"></activity>
        <activity android:name=".communityWrite"></activity>
        <activity android:name=".communityContents"></activity>

        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>